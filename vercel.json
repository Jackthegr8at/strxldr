{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/(.*)\\.(?:png|jpg|jpeg|gif|ico|svg|webp)$",
      "dest": "/public/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}